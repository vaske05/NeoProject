<Window x:Class="NeoProject.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NeoProject"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <CheckBox x:Name="checkBox" Content="CheckBox" HorizontalAlignment="Left" Margin="5,7,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.53,0.508" Checked="checkBox_Checked" Width="132" Height="61">
        <CheckBox.RenderTransform>
            <TransformGroup>
                <ScaleTransform/>
                <SkewTransform/>
                <RotateTransform Angle="1800.226"/>
                <TranslateTransform X="0.004" Y="0.016"/>
            </TransformGroup>
        </CheckBox.RenderTransform>
    </CheckBox>
</Window>
